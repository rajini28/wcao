CREATE OR REPLACE PACKAGE "RAJ_PKG" IS 
FUNCTION TO_NUMBER_RAJINI(IP_NUM VARCHAR2) RETURN NUMBER;
FUNCTION SYSDATE_TOMRW RETURN DATE;
FUNCTION SYSDATE_SPELLEDOUT RETURN VARCHAR2;
PROCEDURE PUT_LINE(IP_PUT VARCHAR2);

END RAJ_PKG;
/

CREATE OR REPLACE PACKAGE BODY "RAJ_PKG" IS

FUNCTION TO_NUMBER_RAJINI(IP_NUM VARCHAR2)
RETURN NUMBER IS
BEGIN
RETURN TO_NUMBER(IP_NUM);
EXCEPTION
WHEN OTHERS THEN
RETURN NULL;
END TO_NUMBER_RAJINI;


FUNCTION SYSDATE_TOMRW
RETURN DATE IS
BEGIN
RETURN 
SYSDATE+1;
END SYSDATE_TOMRW;

FUNCTION SYSDATE_SPELLEDOUT
RETURN VARCHAR2 IS
BEGIN
RETURN
TO_CHAR(SYSDATE,'DD MON YEAR HH24:MI:SS');
END SYSDATE_SPELLEDOUT;

PROCEDURE PUT_LINE(IP_PUT VARCHAR2)
IS
BEGIN
DBMS_OUTPUT.PUT_LINE(IP_PUT);
END PUT_LINE;

END RAJ_PKG;
/





